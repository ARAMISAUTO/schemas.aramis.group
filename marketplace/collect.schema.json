{
    "$id": "https://schemas.aramis.group/marketplace/collect.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "collector": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "partnerName": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                },
                "aramisautoSupplierId": {
                    "type": "integer"
                },
                "aramisautoPickupPointId": {
                    "type": "integer"
                },
                "purchasingVatRate": {
                    "type": "number"
                },
                "disable": {
                    "type": "boolean"
                },
                "collectOnly": {
                    "type": "boolean"
                },
                "language": {
                    "type": "string"
                },
                "format": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "type": {
                            "type": "string"
                        },
                        "escape": {
                            "type": "string"
                        },
                        "enclosure": {
                            "type": "string"
                        },
                        "separator": {
                            "type": "string"
                        }
                    }
                }
            },
            "required": [
                "partnerName",
                "url"
            ]
        }
    },
    "required": [
        "collector"
    ]
}
