{
  "$id": "https://schemas.aramis.group/catalog/vehicle-single-language-searchable-data.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "$schema": { "type": "string", "pattern": "\/catalog\/vehicle-single-language-searchable-data\\.schema\\.json$" },
    "vin": { "type": ["null", "string"], "pattern": "^[A-HJ-NPR-Z0-9]{17}$" },
    "offer_id": { "type": "string", "minLength": 1 },
    "vehicle_ids": { "type": "array", "minItems": 1, "items": { "type": "string", "minLength": 1 } },
    "offer_type_id": { "$ref": "./base.schema.json#/$defs/offer-type-id" },
    "offer_type_label": { "type": "string", "minLength": 1 },
    "status_id": { "$ref": "./base.schema.json#/$defs/status-id" },
    "status_label": { "type": "string", "minLength": 1 },
    "is_marketplace": { "type": "boolean" },
    "first_circulation_date": { "type": ["null","string"], "format": "date", "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$" },
    "first_circulation_year": { "type": ["null","integer"], "minimum": 1800, "maximum": 2100 },
    "publication_date": { "type": "string", "format": "date", "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$" },
    "warranty_expiration_date": { "type": ["null", "string"], "format": "date", "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$" },
    "mileage_km": { "type": "integer", "minimum": 0 },
    "mileage_km_slice": { "type": "integer", "minimum": 0 },
    "euronorm": { "anyOf": [{ "type": "null" }, { "$ref": "./base.schema.json#/$defs/euronorm-string" }] },
    "critair": { "anyOf": [{ "type": "null" }, { "$ref": "./base.schema.json#/$defs/critair-string" }] },
    "french_fiscal_power": { "type": ["null", "integer"], "exclusiveMinimum": 0 },
    "french_genre": { "anyOf": [{ "type": "null" }, { "$ref": "./base.schema.json#/$defs/french-genre-string" }] },
    "belgian_fiscal_power": { "type": ["null", "integer"], "exclusiveMinimum": 0 },
    "belgian_ototo_url": { "type":  ["null", "string"], "format": "uri", "pattern": "^https?://" },
    "belgian_carpass_url": { "type":  ["null", "string"], "format": "uri", "pattern": "^https?://" },
    "catalog_price_with_options_with_taxes": { "type": "number", "exclusiveMinimum": 0 },
    "takeover_premium": { "type": "number", "maximum": 0 },
    "selling_price_with_taxes": { "type": "number", "exclusiveMinimum": 0 },
    "selling_price_with_taxes_slice": { "type": "integer", "minimum": 0 },
    "discount_percent": { "type": "number", "minimum": 0, "maximum": 100 },
    "selling_price_with_takeover_with_taxes": { "type": "number", "exclusiveMinimum": 0 },
    "selling_price_with_takeover_with_taxes_slice": { "type": "integer", "minimum": 0 },
    "discount_with_takeover_percent": { "type": "number", "minimum": 0, "maximum": 100 },
    "indicative_loan_months": { "type": "integer", "exclusiveMinimum": 0 },
    "indicative_loan_monthly_installment": { "type": "number", "exclusiveMinimum": 0 },
    "indicative_loan_monthly_installment_slice": { "type": "integer", "minimum": 0 },
    "maker": { "type": "string", "minLength": 1 },
    "model": { "type": "string", "minLength": 1 },
    "engine": { "type": ["null","string"], "minLength": 1 },
    "finish": { "type": ["null","string"], "minLength": 1 },
    "version": { "type": ["null","string"], "minLength": 1 },
    "interior": { "type": ["null","string"], "minLength": 1 },
    "manufacturer_colors": { "type": "array", "minItems": 0, "items": { "type": "string", "minLength": 1 } },
    "expert_comments": { "type": ["null","string"], "minLength": 1 },
    "media_types": { "type": "array", "items": { "enum": ["photo", "video"] } },
    "media_urls": { "type": "array", "items": { "type": "string", "format": "uri", "pattern": "^https?://" } },
    "media_orientations": { "type": "array", "items": { "type": ["null","integer"], "minimum": 0, "exclusiveMaximum": 360 } },
    "photo_url": { "type": "string", "format": "uri", "pattern": "^https?://" },
    "category_id": { "$ref": "./base.schema.json#/$defs/category-id" },
    "category_label": { "type": "string", "minLength": 1 },
    "segment_id": { "anyOf": [{ "type": "null" },{ "$ref": "./base.schema.json#/$defs/segment-id" }] },
    "segment_label": { "type": ["null","string"], "minLength": 1 },
    "energy_type_ids": { "type": "array", "minItems": 0, "items": { "$ref": "./base.schema.json#/$defs/energy-type-id" } },
    "energy_type_labels": { "type": "array", "minItems": 0, "items": { "type": "string", "minLength": 1 } },
    "simple_color_ids": { "type": "array", "minItems": 1, "items": { "$ref": "./base.schema.json#/$defs/simple-color-id" } },
    "simple_color_labels": { "type": "array", "minItems": 1, "items": { "type": "string", "minLength": 1 } },
    "simple_color2_ids": { "type": "array", "minItems": 1, "items": { "anyOf": [{"type": "null"}, { "$ref": "./base.schema.json#/$defs/simple-color-id" }] } },
    "simple_color2_labels": { "type": "array", "minItems": 1, "items": { "type": ["null","string"], "minLength": 1 } },
    "interior_simple_color_id": { "anyOf": [{"type": "null"}, { "$ref": "./base.schema.json#/$defs/simple-color-id" }] },
    "interior_simple_color_label": { "type": ["null","string"], "minLength": 1 },
    "transmission_id": { "$ref": "./base.schema.json#/$defs/transmission-id" },
    "transmission_label": { "type": "string", "minLength": 1 },
    "drive_id": { "$ref": "./base.schema.json#/$defs/drive-id" },
    "drive_label": { "type": "string", "minLength": 1 },
    "equipment_labels": { "type": "array", "items": { "type": "string", "minLength": 1 } },
    "equipment_category_ids": { "type": "array", "items": { "$ref": "./base.schema.json#/$defs/equipment-category-id" } },
    "equipment_category_labels": { "type": "array", "items": { "type": "string", "minLength": 1 } },
    "simple_equipment_ids": { "type": "array", "minItems": 0, "items": { "$ref": "./base.schema.json#/$defs/simple-equipment-id" } },
    "simple_equipment_labels": { "type": "array", "minItems": 0, "items": { "type": "string", "minLength": 1 } },
    "option_labels": { "type": "array", "items": { "type": "string", "minLength": 1 } },
    "option_price_with_taxes": { "type": "array", "items": { "type": "number" } },
    "promotions_ids": { "type": "array", "items": { "type": "string", "minLength": 1 } },
    "promotions_labels": { "type": "array", "items": { "type": "string", "minLength": 1 } },
    "promotions_colors": { "type": "array", "items": { "type": "string", "pattern": "^[a-fA-F0-9]{6}$" } },
    "doors": { "type": ["null","integer"], "exclusiveMinimum": 0 },
    "gears": { "type": ["null","integer"], "exclusiveMinimum": 0 },
    "seats": { "type": ["null","integer"], "exclusiveMinimum": 0 },
    "cylinders": { "type": ["null","integer"], "exclusiveMinimum": 0 },
    "power_ch": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "power_kw": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "engine_capacity_cc": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "urban_cycle_consumption_lt_per_100km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "urban_cycle_consumption_kwh_per_100km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "extra_urban_cycle_consumption_lt_per_100km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "extra_urban_cycle_consumption_kwh_per_100km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "combined_cycle_consumption_lt_per_100km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "combined_cycle_consumption_kwh_per_100km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "co2_emissions_nedc_grams_per_km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "co2_emissions_wltp_grams_per_km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "height_meters": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "width_meters": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "length_meters": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "wheelbase_meters": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "unloaded_weight_kg": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "total_authorized_weight_kg": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "min_boot_space_back_capacity_lt": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "max_boot_space_back_capacity_lt": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "boot_space_front_capacity_lt": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "max_speed_km_per_hour": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_range_nedc_km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_range_wltp_km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_warranty_period_km": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_warranty_period_years": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_state_of_health_percent": { "type": ["null","number"], "minimum": 0, "maximum": 100 },
    "electric_battery_capacity_kwh": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_type": { "type": ["null","string"], "minLength": 1 },
    "electric_battery_slow_charge_power_kw": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_slow_charge_time_minutes": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_medium_charge_power_kw": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_medium_charge_time_minutes": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_quick_charge_power_kw": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_quick_charge_time_minutes": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_battery_cost": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_maximum_power_car_electrical_outlet_kw": { "type": ["null","number"], "exclusiveMinimum": 0 },
    "electric_maximum_power_car_direct_current_amperes": { "type": ["null","number"], "exclusiveMinimum": 0 }
  },
